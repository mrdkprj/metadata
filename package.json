{
  "name": "win32metadata",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "cargo-cp-artifact -nc ./build/index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "node-pre-gyp install --fallback-to-build",
    "package": "node-pre-gyp package",
    "run-test": "node ./test/test.js",
    "test": "tsc ./test/test.ts --esModuleInterop"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cargo-cp-artifact": "^0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11"
  },
  "binary": {
    "module_name": "index",
    "module_path": "./build/",
    "host": "https://github.com/mrdkprj/metadata/releases/download/",
    "remote_path": "{version}",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  }
}
