{
  "name": "win32props",
  "version": "0.3.0",
  "description": "Reads and writes Windows file property values",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "install": "npm run build-release",
    "build": "cargo-cp-artifact -nc ./build/index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "package": "node-pre-gyp package",
    "test": "node ./test/test.js",
    "tsc": "tsc ./test/test.ts --esModuleInterop --lib ES2015,dom"
  },
  "author": "mrdkprj",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cargo-cp-artifact": "^0.1"
  },
  "binary": {
    "module_name": "index",
    "module_path": "./build/",
    "host": "https://github.com/mrdkprj/metadata/releases/download/",
    "remote_path": "{version}",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  },
  "keywords": [
    "win32,",
    "windows,",
    "metadata,",
    "property,",
    "read,",
    "write,",
    "extract,"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mrdkprj/win32props.git"
  },
  "homepage": "https://github.com/mrdkprj/win32props",
  "bugs": {
    "url": "https://github.com/mrdkprj/win32props/issues"
  }
}
